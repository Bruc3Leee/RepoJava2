<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ClientController">

    <children>
        <Button fx:id="cBtn" onAction="#getSend" layoutX="500.0" layoutY="340.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="75.0" text="Send" />
        <TextField fx:id="cTextField" onKeyPressed="#presSend" layoutX="25.0" layoutY="340.0" prefHeight="40.0" prefWidth="450.0" />
        <TextArea fx:id="cTextArea" layoutX="25.0" layoutY="25.0" prefHeight="300.0" prefWidth="550.0" editable="false"/>
    </children>
</AnchorPane>